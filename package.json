{
  "name": "@lms/lms-front",
  "author": "Market academy",
  "version": "1.0.0",
  "description": "Typescript Version",
  "private": true,
  "scripts": {
    "dev": "yarn run vite -- --port 30000 --host",
    "test": "react-scripts test",
    "test:all": "yarn test --bail --watchAll=false --coverage --findRelatedTests",
    "test:ci": "yarn test --bail --watchAll=false --coverage --testResultsProcessor=\"jest-junit\"",
    "build": "vite build",
    "preview": "vite preview",
    "extract": "lingui extract",
    "compile": "lingui compile --typescript",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "yarn test:all",
      "yarn extract",
      "yarn compile",
      "git-format-staged --formatter 'prettier --stdin-filepath \"{}\"' 'src/*.ts' 'src/*.tsx'",
      "git add src/locales"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@iconify/react": "^4.1.1",
    "@lingui/detect-locale": "^4.7.0",
    "@lingui/format-json": "^4.7.0",
    "@lingui/react": "^4.7.0",
    "@mui/lab": "^5.0.0-alpha.109",
    "@mui/material": "^5.15.11",
    "@mui/system": "^5.10.15",
    "@mui/x-data-grid": "^7.1.1",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.7",
    "date-fns": "^3.6.0",
    "lodash.transform": "^4.6.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.39.5",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "web-vitals": "^3.1.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@lingui/cli": "^4.7.0",
    "@lingui/macro": "^4.7.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/dom": "7.21.4",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "7.1.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "axios-mock-adapter": "^1.22.0",
    "babel-plugin-macros": "^3.1.0",
    "csstype": "^3.0.10",
    "eslint": "^8.55.0",
    "git-format-staged": "^3.1.1",
    "husky": "^9.0.11",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.{ts,tsx}",
      "<rootDir>/src/pages/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "locales",
      "tests",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/App.tsx"
    ],
    "testMatch": [
      "<rootDir>/src/tests/**/*.test.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@src(.*)$": "<rootDir>/src$1",
      "^@redux/(.*)$": "<rootDir>/src/redux/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "@testProvider": "<rootDir>/src/testProvider.tsx",
      "^axios$": "axios/dist/node/axios.cjs"
    }
  }
}
